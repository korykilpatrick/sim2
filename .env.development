# Development Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:3001/api/v1
VITE_WEBSOCKET_URL=ws://localhost:3001

# Authentication
VITE_AUTH_TOKEN_KEY=sim_auth_token
VITE_REFRESH_TOKEN_KEY=sim_refresh_token

# Error Tracking (Sentry) - Disabled in development
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development
VITE_SENTRY_TRACES_SAMPLE_RATE=0

# Analytics - Disabled in development
VITE_ANALYTICS_ID=
VITE_ANALYTICS_ENABLED=false

# Feature Flags
VITE_ENABLE_MOCK_API=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_WEBSOCKET=true

# Map Configuration
VITE_MAPBOX_ACCESS_TOKEN=
VITE_DEFAULT_MAP_CENTER_LAT=30
VITE_DEFAULT_MAP_CENTER_LNG=-40
VITE_DEFAULT_MAP_ZOOM=3

# External Services
VITE_STRIPE_PUBLIC_KEY=
VITE_INTERCOM_APP_ID=

# Application Settings
VITE_APP_NAME="SIM - Synmax Intelligence for Maritime"
VITE_SESSION_TIMEOUT_MINUTES=120
VITE_API_TIMEOUT_MS=30000

# Development Settings
VITE_DEV_SHOW_REDUX_DEVTOOLS=true
VITE_DEV_SHOW_QUERY_DEVTOOLS=true